    <nav>
        <ul class="nav-bar">
        <li class="nav-list"><a href="/">Home</a></li>
        <li class="nav-list"><a href="/venues">Search Venues</a></li>
        <li class="nav-list"><a href="/artists">Search Artists</a></li>
