<main>


    <h1><%= artist.alias %></h1><br>
    <img width="400" height="250" src="<%= artist.url%>"><br>
    <iframe width="400" height="250" src="<%=artist.link%>"></iframe><br>
    Bio:<%= artist.bio %><br>
    Booking Info:<%= artist.bookingInfo %><br>
    Genre:<%= artist.genre %><br>
    City:<%= artist.city %><br>

    <a class="editBtn" href="/artists/<%= artist._id %>/edit">Edit Artist Profile</a>

    <form action="/artists/<%= artist._id %>?_method=DELETE" method="POST">
    <button type="submit">Delete Artist Profile</button>
</form>

<a href="/">Home</a>

</main>






