<%- include('../partials/header') %>


<main>

    <h1><%= venue.name %></h1><br>
    <img width="400" height="250" src="<%= venue.url%>"><br>
    Info:<%= venue.gigInfo %><br>
    Address:<%= venue.address %><br>
    Pay:<%= venue.pay %><br>

    <a class="editBtn" href="/venues/<%= venue._id %>/edit">Edit Venue Profile</a>

    <form action="/venues/<%= venue._id %>?_method=DELETE" method="POST">
    <button type="submit">Delete Venue Profile</button>
</form>

<a href="/">Home</a>

</main>







